{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "author": {
    "email": "sinashafiee@hotmail.com",
    "name": "Sina Shafiee",
    "url": "https://github.com/sina-shafiee"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint:ci": "eslint --ext \".tsx,.ts\" --ignore-path .eslintignore .",
    "lint": "pnpm run lint:ci --max-warnings 0 --fix",
    "format": "prettier --write",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@next/third-parties": "14.2.13",
    "@sentry/nextjs": "^8",
    "next": "14.2.8",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@commitlint/cli": "19.4.1",
    "@commitlint/config-conventional": "19.4.1",
    "@fullstacksjs/eslint-config": "10.11.1",
    "@storybook/addon-essentials": "8.3.0",
    "@storybook/addon-interactions": "8.3.0",
    "@storybook/addon-links": "8.3.0",
    "@storybook/addon-onboarding": "8.3.0",
    "@storybook/blocks": "8.3.0",
    "@storybook/nextjs": "8.3.0",
    "@storybook/react": "8.3.0",
    "@storybook/test": "8.3.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-istanbul": "2.0.5",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.57.0",
    "eslint-plugin-storybook": "0.8.0",
    "husky": "9.1.5",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "storybook": "8.3.0",
    "typescript": "5.3.3",
    "vite-plugin-env-compatible": "2.0.1",
    "vitest": "2.0.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
